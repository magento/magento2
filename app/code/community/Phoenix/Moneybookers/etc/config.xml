<config>
    <modules>
        <Phoenix_Moneybookers>
            <version>1.6.0.0</version>
        </Phoenix_Moneybookers>
    </modules>
    <global>
        <resources>
            <moneybookers_setup>
                <setup>
                    <module>Phoenix_Moneybookers</module>
                </setup>
            </moneybookers_setup>
        </resources>
        <template>
            <email>
                <moneybookers_activateemail translate="label" module="Phoenix_Moneybookers">
                    <label>Activation of Payment Configuration</label>
                    <file>email/activation.html</file>
                    <type>html</type>
                </moneybookers_activateemail>
            </email>
        </template>
        <payment>
            <groups>
                <mbookers>Moneybookers</mbookers>
            </groups>
        </payment>
    </global>
    <frontend>
        <secure_url>
            <moneybookers_processing>/moneybookers/processing</moneybookers_processing>
        </secure_url>
        <routers>
            <moneybookers>
                <use>standard</use>
                <args>
                    <module>Phoenix_Moneybookers</module>
                    <frontName>moneybookers</frontName>
                </args>
            </moneybookers>
        </routers>
        <translate>
            <modules>
                <Phoenix_Moneybookers>
                    <files>
                        <default>Phoenix_Moneybookers.csv</default>
                    </files>
                </Phoenix_Moneybookers>
            </modules>
        </translate>
        <layout>
            <updates>
                <moneybookers module="Phoenix_Moneybookers">
                    <file>layout.xml</file>
                </moneybookers>
            </updates>
        </layout>
    </frontend>
    <admin>
         <routers>
            <adminhtml>
                <args>
                    <modules>
                        <moneybookers after="Mage_Adminhtml">Phoenix_Moneybookers</moneybookers>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <translate>
            <modules>
                <Phoenix_Moneybookers>
                    <files>
                        <default>Phoenix_Moneybookers.csv</default>
                    </files>
                </Phoenix_Moneybookers>
            </modules>
        </translate>
        <layout>
            <updates>
                <moneybookers module="Phoenix_Moneybookers">
                    <file>layout.xml</file>
                </moneybookers>
            </updates>
        </layout>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <moneybookers translate="title" module="Phoenix_Moneybookers">
                                            <title>Moneybookers Settings</title>
                                        </moneybookers>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
    <phoenix>
        <moneybookers>
            <verify_url>http://www.phoenix-medien.de/index.php?id=105</verify_url>
        </moneybookers>
    </phoenix>
    <default>
        <payment>
            <moneybookers_acc>
                <group>mbookers</group>
                <active>0</active>
                <model>Phoenix_Moneybookers_Model_Acc</model>
                <title>Credit Card / Visa, Mastercard, AMEX, JCB, Diners</title>
                <allowspecific>0</allowspecific>
            </moneybookers_acc>
            <moneybookers_csi>
                <group>mbookers</group>
                <active>0</active>
                <model>Phoenix_Moneybookers_Model_Csi</model>
