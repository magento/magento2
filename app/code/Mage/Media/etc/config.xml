<config>
    <modules>
        <Mage_Media>
            <version>1.6.0.0</version>
            <active>true</active>
            <depends>
                <Mage_Core/>
            </depends>
        </Mage_Media>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Media>
                    <files>
                        <default>Mage_Media.csv</default>
                    </files>
                </Mage_Media>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <routers>
            <media>
                <use>standard</use>
                <args>
                    <module>Mage_Media</module>
                    <frontName>media</frontName>
                </args>
            </media>
        </routers>
        <translate>
            <modules>
                <Mage_Media>
                    <files>
                        <default>Mage_Media.csv</default>
                    </files>
                </Mage_Media>
            </modules>
        </translate>
    </frontend>
</config>
