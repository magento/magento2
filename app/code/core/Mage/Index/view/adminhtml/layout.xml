<layout>
    <default>
        <reference name="notifications">
            <block type="Mage_Index_Block_Adminhtml_Notifications" name="index_notifications" template="notifications.phtml"></block>
        </reference>
    </default>

    <adminhtml_process_edit>
        <reference name="content">
            <block type="Mage_Index_Block_Adminhtml_Process_Edit" name="process_edit"></block>
        </reference>
        <reference name="left">
            <block type="Mage_Index_Block_Adminhtml_Process_Edit_Tabs" name="index_edit_tabs">
                <action method="addTab"><name>main_section</name><block>Mage_Index_Block_Adminhtml_Process_Edit_Tab_Main</block></action>
            </block>
        </reference>
    </adminhtml_process_edit>
</layout>
