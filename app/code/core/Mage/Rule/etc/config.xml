<config>
    <modules>
        <Mage_Rule>
            <version>1.6.0.0</version>
            <active>true</active>
            <codePool>core</codePool>
            <depends>
                <Mage_Core/>
            </depends>
        </Mage_Rule>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Rule>
                    <files>
                        <default>Mage_Rule.csv</default>
                    </files>
                </Mage_Rule>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Mage_Rule>
                    <files>
                        <default>Mage_Rule.csv</default>
                    </files>
                </Mage_Rule>
            </modules>
        </translate>
    </frontend>
</config>
