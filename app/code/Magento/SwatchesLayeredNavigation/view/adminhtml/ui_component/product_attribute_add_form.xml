<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="front_fieldset">
        <field name="is_filterable">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="valuesForEnable" xsi:type="array">
                        <item name="swatch_text" xsi:type="string">swatch_text</item>
                        <item name="swatch_visual" xsi:type="string">swatch_visual</item>
                    </item>
                </item>
            </argument>
        </field>
        <field name="is_filterable_in_search">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="valuesForEnable" xsi:type="array">
                        <item name="swatch_text" xsi:type="string">swatch_text</item>
                        <item name="swatch_visual" xsi:type="string">swatch_visual</item>
                    </item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>
