<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_CurrencySymbol::system_currency" title="Currency" translate="title" sortOrder="30">
                        <resource id="Magento_CurrencySymbol::currency_rates" title="Currency Rates" translate="title" sortOrder="10" />
