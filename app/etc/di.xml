        <param name="eventDispatcher">
            <instance type="Magento\Core\Model\Event\Manager\Proxy" />
        </param>
    </type>
    <type name="Magento\Core\Model\ModuleManager">
        <param name="config">
            <instance type="Magento\Core\Model\Config\Proxy" />
        </param>
    </type>
