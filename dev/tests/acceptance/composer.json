{
    "description": "Magento 2 (Open Source) Functional Tests",
    "type": "project",
    "version": "1.0.0-dev",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:magento/magento2-functional-testing-framework.git"
        }
    ],
    "require": {
        "allure-framework/allure-codeception": "~1.2.6",
        "codeception/codeception": "~2.3.4",
        "consolidation/robo": "^1.0.0",
        "symfony/process": ">=2.7 <3.4",
        "henrikbjorn/lurker": "^1.2",
        "magento/magento2-functional-testing-framework": "~2.1.1",
        "php": "7.0.2|7.0.4|~7.0.6|~7.1.0",
        "vlucas/phpdotenv": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Magento\\": "tests/functional/Magento"
        }
    },
    "prefer-stable": true
}
