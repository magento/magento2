            <constraint name="Magento\Backend\Test\Constraint\AssertBackendPageIsAvailable"/>
        </variation>
        <variation name="NavigateMenuTest47">
            <data name="menuItem" xsi:type="string">Marketing > Newsletter Queue</data>
