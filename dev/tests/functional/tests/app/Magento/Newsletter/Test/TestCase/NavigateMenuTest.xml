            <constraint name="Magento\Backend\Test\Constraint\AssertBackendPageIsAvailable"/>
        </variation>
        <variation name="NavigateMenuTest48">
            <data name="menuItem" xsi:type="string">Marketing > Newsletter Subscribers</data>
            <data name="pageTitle" xsi:type="string">Newsletter Subscribers</data>
            <constraint name="Magento\Backend\Test\Constraint\AssertBackendPageIsAvailable"/>
        </variation>
        <variation name="NavigateMenuTest49">
            <data name="menuItem" xsi:type="string">Reports > Newsletter Problem Reports</data>
