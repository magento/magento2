<mapping strict="0">
    <fields>
        <baseUrl>
            <selector>[name="base_url"]</selector>
        </baseUrl>
        <admin />
        <keyOwn>
            <selector>[value="user"]</selector>
            <input>radiobutton</input>
        </keyOwn>
        <keyValue>
            <selector>[name="key"]</selector>
        </keyValue>
        <apacheRewrites>
            <selector>[ng-model*="rewrites"]</selector>
            <input>checkbox</input>
        </apacheRewrites>
        <httpsFront>
            <selector>[ng-model*="front"]</selector>
            <input>radiobutton</input>
        </httpsFront>
        <https />
        <httpsAdmin>
            <selector>[type="checkbox"][ng-model*="admin"]</selector>
            <input>radiobutton</input>
        </httpsAdmin>
    </fields>
</mapping>
