<mapping strict="1">
    <fields>
        <page_size>
            <selector>.control [name="parameters[page_size]"]</selector>
        </page_size>
        <show_attributes>
            <selector>
                .control [name^="parameters[show_attributes]["]
            </selector>
            <input>multiselect</input>
        </show_attributes>
        <show_buttons>
            <selector>
                .control [name^="parameters[show_buttons]["]
            </selector>
            <input>multiselect</input>
        </show_buttons>
    </fields>
</mapping>
