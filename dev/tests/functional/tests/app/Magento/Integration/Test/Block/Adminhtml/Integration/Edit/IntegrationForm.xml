<tabs>
    <integration_info>
        <class>\Magento\Backend\Test\Block\Widget\Tab</class>
        <selector>#integration_edit_tabs_info_section</selector>
        <strategy>css selector</strategy>
        <fields>
            <name />
            <email />
            <endpoint />
            <identity_link_url />
            <key>
                <selector>[name="consumer_key"]</selector>
            </key>
            <consumer_secret />
            <token />
            <token_secret />
        </fields>
    </integration_info>
    <api>
        <class>\Magento\Backend\Test\Block\Widget\Tab</class>
        <selector>#integration_edit_tabs_api_section</selector>
        <strategy>css selector</strategy>
        <fields>
            <resource_access>
                <selector>[name='all_resources']</selector>
                <strategy>css selector</strategy>
                <input>select</input>
            </resource_access>
            <resources>
                <selector>[data-role="resource-tree"]</selector>
                <strategy>css selector</strategy>
                <input>jquerytree</input>
            </resources>
        </fields>
    </api>
</tabs>
