<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../etc/global/variations.xsd">
    <testCase name="CreateIntegrationEntityTest">
        <variation name="CreateIntegrationEntityTestVariation1">
            <data name="integration/data/name">Integration%isolation%</data>
            <data name="integration/data/email">test@example.com</data>
            <data name="integration/data/endpoint">https://endpoint.com</data>
            <data name="integration/data/identity_link_url">https://testlink.com</data>
            <data name="integration/data/resource_access">All</data>
            <data name="constraint">assertIntegrationSuccessSaveMessage, assertIntegrationForm, assertIntegrationInGrid</data>
        </variation>
        <variation name="CreateIntegrationEntityTestVariation2">
            <data name="integration/data/name">Integration%isolation%</data>
            <data name="integration/data/resource_access">Custom</data>
            <data name="integration/data/resources">Sales</data>
            <data name="constraint">assertIntegrationSuccessSaveMessage, assertIntegrationForm, assertIntegrationInGrid</data>
        </variation>
        <variation name="CreateIntegrationEntityTestVariation3">
            <data name="integration/data/name">Integration%isolation%</data>
            <data name="integration/data/resource_access">All</data>
            <data name="constraint">assertIntegrationSuccessSaveMessage, assertIntegrationForm, assertIntegrationInGrid</data>
        </variation>
    </testCase>
</config>
