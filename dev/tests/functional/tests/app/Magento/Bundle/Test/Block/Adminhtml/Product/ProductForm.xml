        <selector>#product_info_tabs_product-details</selector>
        <strategy>css selector</strategy>
        <wrapper>product</wrapper>
        <fields>
            <shipment_type>
                <input>select</input>
            </shipment_type>
            <sku_type>
                <input>select</input>
            </sku_type>
            <price_type>
                <input>select</input>
            </price_type>
